# START_CONFIT_GENERATED_CONTENT
extends: defaults/configurations/google

env:
  jasmine: true
  node: true

  commonjs: true    # For Webpack, CommonJS

  es6: true
  mocha: true

confit-globals: &confit-globals

ecmaFeatures:
  modules: true

# END_CONFIT_GENERATED_CONTENT
globals:
  <<: *confit-globals

rules:
  max-len:
    - 2       # Warning
    - 200     # Line Length

  dot-location:
    - 1       #error
    - property

  no-unused-vars:
    - 1
